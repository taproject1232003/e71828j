{
  "name": "minapp-sdk",
  "version": "2.0.0-a",
  "main": "./lib/index.js",
  "browser": "./lib/web.js",
  "miniprogram": "lib",
  "description": "爱范儿知晓云平台 JS SDK",
  "scripts": {
    "test": "npm run test-wechat",
    "test-wechat": "mocha test/wechat/index.js",
    "test-web": "anywhere -p 40034 -h localhost -d test/web",
    "dev": "export NODE_ENV=dev && webpack --config sdk-file/webpack.config.js --mode development -w",
    "build": "webpack --config sdk-file/webpack.config.js --mode production",
    "build-npm": "npm run build",
    "lint": "eslint core/**/*.js sdk-file/src/**/*.js",
    "prepare": "npm run build-npm",
    "prepublishOnly": "npm run lint",
    "prepublish": "npm run build-npm && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ifanrx/repository.git"
  },
  "keywords": [
    "ifanrx"
  ],
  "author": "ifanrx",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ifanrx/repository/issues"
  },
  "homepage": "https://github.com/ifanrx/repository#readme",
  "versions": {
    "alipay": "2.0.0-a",
    "web": "2.0.0-a"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "anywhere": "^1.5.0",
    "babel-loader": "^8.0.5",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint": "4.19.1",
    "faker": "^4.1.0",
    "mocha": "^3.2.0",
    "mochawesome": "^2.2.0",
    "node-localstorage": "^1.3.0",
    "rewire": "^2.5.2",
    "sinon": "^3.2.0",
    "sinon-stub-promise": "^4.0.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1"
  },
  "dependencies": {
    "axios": "^0.18.0"
  }
}
